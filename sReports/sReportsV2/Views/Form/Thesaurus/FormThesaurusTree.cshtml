@using sReportsV2.DTOs.Form.DataOut.Tree

@model TreeDataOut

@if (Model.Forms.Count == 0)
{
    @await Html.PartialAsync("NoResultFound")
}
else
{
    <div class="found-in-count" id="thesaurusAppearances-@Model.O4MtId">@ViewBag.TotalAppeareance</div>

    <div class="form-tree" id="foundInContainer-@Model.O4MtId">
        @await Html.PartialAsync("Thesaurus/FormThesaurusTreePartial")
    </div>
}